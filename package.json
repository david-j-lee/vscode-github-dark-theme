{
  "name": "github-dark-color-theme",
  "displayName": "GitHub Dark Color Theme",
  "description": "GitHub inspired dark theme.",
  "version": "0.4.3",
  "publisher": "david-j-lee",
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/david-j-lee/vscode-github-dark-theme"
  },
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "GitHub Dark (Web Based)",
        "uiTheme": "vs-dark",
        "path": "./themes/github-dark-color-theme.json"
      },
      {
        "label": "GitHub Dark (Web Based Borderless)",
        "uiTheme": "vs-dark",
        "path": "./themes/github-dark-color-borderless-theme.json"
      }
    ]
  },
  "scripts": {
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "build": "tsx ./src/index.js",
    "package": "npm run build && vsce package",
    "publish": "npm run package & vsce publish"
  },
  "dependencies": {
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  }
}
